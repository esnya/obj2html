{
  "name": "obj2html",
  "version": "0.3.5",
  "description": "Convert 3D models of  Wavefront obj to HTML using CSS3 transforms.",
  "keywords": [
    "3d",
    "convert",
    "css",
    "html",
    "obj"
  ],
  "homepage": "https://github.com/ukatama/obj2html#readme",
  "bugs": {
    "url": "https://github.com/ukatama/obj2html/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ukatama/obj2html.git"
  },
  "license": "MIT",
  "author": "ukatama",
  "main": "dist",
  "bin": "bin.js",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "ci:before_deploy": "npm run production:build",
    "ci:script": "run-s lint test build",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint --ext .ts --fix ts tests",
    "production:build": "cross-env NODE_ENV=production npm run build",
    "start": "ts-node-dev ts/cli.ts",
    "test": "nyc mocha --require ts-node/register tests/**/*.ts",
    "watch": "ts-node-dev --respawn ts/cli.ts"
  },
  "dependencies": {
    "jsdom": "^13.2.0",
    "loader-utils": "^1.2.3",
    "lodash": "^4.17.11",
    "math.gl": "^2.3.0",
    "parse5": "^5.1.0",
    "shortid": "^2.2.14",
    "yargs": "^13.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/jsdom": "^12.2.2",
    "@types/loader-utils": "^1.1.3",
    "@types/lodash": "^4.14.121",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.4",
    "@types/shortid": "0.0.29",
    "@types/yargs": "^12.0.9",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.3.0",
    "ts-node": "^8.0.2",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.3.3"
  },
  "nyc": {
    "include": [
      "ts/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true
  }
}

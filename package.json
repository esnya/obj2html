{
  "name": "obj2html",
  "version": "0.3.0",
  "description": "Convert 3D models of  Wavefront obj to HTML using CSS3 transforms.",
  "keywords": [
    "3d",
    "convert",
    "css",
    "html",
    "obj"
  ],
  "homepage": "https://github.com/ukatama/obj2html#readme",
  "bugs": {
    "url": "https://github.com/ukatama/obj2html/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ukatama/obj2html.git"
  },
  "license": "MIT",
  "author": "ukatama",
  "main": "dist",
  "bin": "bin.js",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "lint": "eslint --ext .ts --fix ts",
    "prepublish": "run-s lint production:build",
    "production:build": "cross-env NODE_ENV=production npm run build",
    "start": "ts-node-dev ts/cli.ts",
    "watch": "ts-node-dev --respawn ts/cli.ts"
  },
  "dependencies": {
    "jsdom": "^13.2.0",
    "loader-utils": "^1.2.3",
    "lodash": "^4.17.11",
    "math.gl": "^2.3.0",
    "parse5": "^5.1.0",
    "shortid": "^2.2.14",
    "yargs": "^13.1.0"
  },
  "devDependencies": {
    "@types/jsdom": "^12.2.1",
    "@types/lodash": "^4.14.120",
    "@types/node": "^11.9.0",
    "@types/yargs": "^12.0.8",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.0.2",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.3.3"
  }
}
